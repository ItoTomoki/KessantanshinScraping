<script type="text/javascript">
jQuery.noConflict();
var $ = jQuery;
$(document).ready(function(){
    $("#g-totop").hide();
    $(function () {
        $(window).scroll(function () {
            if ($(this).scrollTop() > 0) {
                $('#g-totop').fadeIn(1000);
            } else {
                $('#g-totop').fadeOut(1000);
            }
        }); 
    $('a#g-totop').click(function () {
        $('body').animate({
        scrollTop: 0
        }, 80);
        return false;
         });
    });
});
</script>




