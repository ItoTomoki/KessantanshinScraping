        <script type="text/javascript">
$(function(){
var imagetag = '<img alt="PDF" src="/assets/templates/hibiya/ir/images/pdf.gif"/>';

// 以下の領域の a タグで、href の末尾が pdf で終わっているものに対して、pdfアイコンを付け足します
$('div#col03 a, div#page-content a').each( function(){
	if ( $(this).attr('href').match(/.*\.pdf$/) != null ){
		$(this).append(imagetag);}
});

});
</script>


