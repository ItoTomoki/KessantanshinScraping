//メガメニュー表示

var megaHeight=0;
$(function (){
	$(".mega .inner").css("overflow","hidden");
	$(window).load(function(){
		$(".mega").show();

		megaHeight=$(".mega .inner").height();
		$(".mega .inner").height(0);
		$(".mega").hide();



	$(".lineuparea .lineup a").bind("mouseover",function(){
			openMega();
	})

	$(".lineuparea .lineup a").bind("mouseout",function(){
			closeMega();
	});

	$(".lineuparea .mega").bind("mouseover",function(){
			openMega();
	});
	$(".lineuparea .mega").bind("mouseout",function(){
			closeMega();
	});
		})

})

function openMega(){
	if(!closeMotion){
		if(!(BrowserInfo.ie && BrowserInfo.ieVersion<=7))$("#modal").show();
		$("#modal").stop(true).animate({opacity:1},200,"linear");
		$(".mega").show();
		$(".lineuparea .lineup a").addClass("active");
		$(".mega .inner").stop(true).animate({height:megaHeight},400,"easeOutQuart");
	}
}

function closeMega(){
	if(!closeMotion){
		$("#modal").hide();
		$("#modal").stop(true).animate({opacity:0},200,"linear");
		$(".mega .inner").stop(true).animate({height:0},400,"easeOutQuart",function(){
			$(".lineuparea .lineup a").removeClass("active");
			$(".mega").hide();
		});
	}
}

var closeMotion=false;
function closeMega2(){
	closeMotion=true;
	$("#modal").hide();
	$("#modal").stop(true).animate({opacity:0},200,"linear");
	$(".mega .inner").stop(true).animate({height:0},400,"easeOutQuart",function(){
		$(".lineuparea .lineup a").removeClass("active");
		$(".mega").hide();
		closeMotion=false;
	});
	/**/
}

