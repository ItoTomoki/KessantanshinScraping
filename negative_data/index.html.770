        paginationClickable: false,　// ページネーションマーカーをクリックしてスライドを切り替える
        autoHover: true, // マウスオーバー時に自動遷移を停止する
        touchEnabled: true // スワイプでもスライドするようにする
        // paginationType: 'fraction' // ページネーションを数字にする
    });
    </script>
</body>
</html>