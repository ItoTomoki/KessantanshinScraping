			function news_fixed(){
				if ($(window).width() > 767) {
					var phn_h = $('#pc_header_nav').height();
					var tar = $('#maincontens').offset().top  - $('#header_in').height() - phn_h;

					if ($(window).scrollTop() >= tar) {
						$('#pc_header_nav').addClass('nav_fixed');
						$('#pc_header_nav').removeClass('nav_not_fixed');
						$('#pc_header_nav_ul').addClass('down');
						$('#pc_header_nav_ul').removeClass('up');
						$('#maincontens').css('margin-top', phn_h);
					}else{
						$('#pc_header_nav').addClass('nav_not_fixed');
						$('#pc_header_nav').removeClass('nav_fixed');
						$('#pc_header_nav_ul').addClass('up');
						$('#pc_header_nav_ul').removeClass('down');
						$('#maincontens').css('margin-top', 0);
					}
				}
			}

			news_fixed();

