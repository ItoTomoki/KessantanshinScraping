<script>
(function(){
  var InitFeatureSlider = function () {
    this.contrW = 950;
    this.sliderItmW = 488;
    this.$slideWrap = $('[data-toggle="featureSlider"]');
    this.$sliderBod = $('[data-toggle="featureSlider"] .sliderCont');
    this.$sliderMaskPrev = $('<div class="sliderMaskPrev" />');
    this.$sliderMaskNext = $('<div class="sliderMaskNext" />');
    this.$slideWrap.append(this.$sliderMaskPrev);
    this.$slideWrap.append(this.$sliderMaskNext);
    this.sliderMaskMinW = ( this.contrW - this.sliderItmW ) / 2

    this.init();
  }
  InitFeatureSlider.prototype.init = function () {
    var self = this;
    this.$sliderBod.slick({
      infinite: true,
      speed: 300,
      autoplay: true,
      autoplaySpeed: 3000,
      slidesToShow: 1,
      centerMode: true,
      variableWidth: true,
      prevArrow: '.sliderPrev',
      nextArrow: '.sliderNext',
      touchThreshold: 20,
      appendDots: '.sliderDotsWrap',
      dots: true,
      dotsClass: 'sliderDots'
    });

    this.setMaskWidth();
    $(window).on('resize', function () {
      self.setMaskWidth();
    } );
  };
  InitFeatureSlider.prototype.setMaskWidth = function () {
    var w = this.$slideWrap.width() / 2 - this.sliderItmW / 2;
    if (this.sliderMaskMinW > w) w = this.sliderMaskMinW;
    this.$sliderMaskPrev.css({'width': w + 'px'});
    this.$sliderMaskNext.css({'width': w + 'px'});
  };
  new InitFeatureSlider();
})();
</script>

