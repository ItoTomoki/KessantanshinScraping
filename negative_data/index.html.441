$.ajax({
    url: "/ajax/pickup.php",
    dataType: 'html'
}).done(function( data ) {
    if (data == '') {
        remove_pickup();
    } else {
        if ($('.pickup_content').length > 0) {
            $('.pickup_content').append(data);
            $('.pickup_box').slick({
                slidesToShow: 4,
                arrows: true,
                responsive: [
                    {
                        breakpoint: 767,
                        settings: {
                            arrows: true,
                            centerMode: true,
                            slidesToShow: 1,
                            slidesToScroll: 1,
                        }
                    }
                ]
            });
        }
    }
}).fail(function() {
    remove_pickup();
});
function remove_pickup() {
    $('.pickup_content').parents('section').remove();
}
