  .pickup-slider .caption,
  .pickup-slider .title {
    font-size: 13px !important;
  }
  .pickup-slider .lead {
    margin: 10px auto;
    font-size: 11px !important;
  }
  .pickup-slider .bx-controls-direction {
    display: none;
  }
}
