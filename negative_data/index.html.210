/** ページ固有の実装だったので、直書きしました。cssへ移動しても問題はないです。 */
.pickup-slider {
  -webkit-text-size-adjust: 100%; 
  position: relative;
  border: 2px solid #d1ad5a;
  margin-bottom: 40px;
}
/** 打ち消しスタイル { */
.pickup-slider .bx-wrapper {
  max-width: 100%;
  margin-bottom: 0;
  box-shadow: none;
}
.pickup-slider .bx-wrapper .bx-pager.bx-default-pager a {
  border: 1px solid #c49832;
  background: #fff;
  border-radius: 6px;
}
.pickup-slider .bx-wrapper .bx-pager.bx-default-pager a.active {
  background: #c49832;
}
.pickup-slider .bx-wrapper .bx-controls-direction a {
  width: 20px;
  height: 37px;
}
.pickup-slider .bx-wrapper .bx-controls-direction a:hover {
  opacity: 0.6;
}
.pickup-slider .bx-wrapper .bx-prev {
  background: url(/slider_controls.png) no-repeat 0 0px;
}
.pickup-slider .bx-wrapper .bx-next {
  background: url(/slider_controls.png) no-repeat -55px 0px;
}
/** } 打ち消しスタイル */
.pickup-slider:after {
  content: "";
  position: absolute;
  left: 0;
  top: 0;
  display: block;
  width: 120px;
  height: 85px;
  background-image: url("../images/index/icon_pickup.png");
}
.pickup-slider__container {
}
.pickup-slider__container li {
  float: left;
}
.pickup-slider__container:after {
  content: ' ';
  clear: both;
}
.pickup-slider__contents {
  display: block;
  color: #4d4d4d !important;
  text-decoration: none;
}
.pickup-slider__contents:after {
  content: ' ';
  clear: both;
}
.pickup-slider .image {
  position: relative;
  width: 578px;
  float: left;
}
.pickup-slider .property {
  width: 340px;
  float: right;
  margin-right: 40px;
}
.pickup-slider .logo {
  margin: 10px auto;
}
.pickup-slider .logo img {
  display: block;
  width: 214px;
  margin: auto;
}
.pickup-slider .text {
  line-height: 1.6;
  text-align: center;
}
.pickup-slider .caption, 
.pickup-slider .title {
  font-size: 18px;
}
.pickup-slider .lead {
  margin: 20px auto 0;
  font-size: 14px;
}
@media screen and (max-width: 639px) {
  .pickup-slider .image {
    width: auto;
    float: none;
  }
  .pickup-slider .image img {
    width: 100%;
    height: auto;
