<script>
(function(window, $){
$(function(){
var path = 'shared/img/contents/kv_text0';
var max = 5;
var index = 1;
// control video
$('video').prop('muted', true);
function doAnim(prevnext) {
if (prevnext == 'next') {
index = (index >= max ? 1 : index + 1);
} else {
index = (index <= 1 ? max : index - 1);
}
$('#slideshow .copy')
.hide()
.find('img').attr('src', path + index + '.png').end()
.fadeIn('slow');
}
var timer = setInterval(function(){
doAnim('next');
}, 9000);
$('#next').click(function() {
doAnim('next');
clearInterval(timer);
});
$('#prev').click(function() {
doAnim('prev');
clearInterval(timer);
});
var $tabNav = $('#tab-nav'),
topics = 'http://ir.dreamv.co.jp/feed/news/news.xml',
ir = 'http://ir.dreamv.co.jp/feed/news/ir.xml';
$tabNav.find('.group a').on('click', null, function(ev){
ev.preventDefault();
var url = topics;
if(/IR News/.test($(this).text())){
url = ir
}
$tabNav.find('.rss a').attr('href', url);
});
});
})(window, jQuery);

(function($) {
	var objBlock = $('.mod-block');
	var objTab1 = $('#mod-block-tab1');
	var objTab2 = $('#mod-block-tab2');
	var objTabBody1 = $('#mod-block-tabBody1');
	var objTabBody2 = $('#mod-block-tabBody2');
	objTabBody2.hide();
	objTab1.click(function(e){
		objBlock.find('li').removeClass('active');
		$(this).addClass('active');
		e.preventDefault();
		objTabBody1.hide();
		objTabBody2.hide();
		objTabBody1.show();
	});
	objTab2.click(function(e){
		objBlock.find('li').removeClass('active');
		$(this).addClass('active');
		e.preventDefault();
		objTabBody1.hide();
		objTabBody2.hide();
		objTabBody2.show();
	});
})(jQuery);
</script>

