      jQuery.noConflict();
      if ((navigator.userAgent.indexOf('iPhone') > 0 && navigator.userAgent.indexOf('iPad') == -1) || navigator.userAgent.indexOf('iPod') > 0 || navigator.userAgent.indexOf('Android') > 0) {
      }
      else{
        jQuery(window).load(function(){
          if( (jQuery.browser.msie && jQuery.browser.version > 6) || !jQuery.browser.msie){
            jQuery('.floating').meerkat({
              position: 'right',
              close: '.floatingClose',
              animationIn: 'slide',
              animationOut: 'none'
            })
            setTimeout(function() {
              jQuery('.floating').addClass('slideImg');
              jQuery('.floatingImg').animate({'right': '-170px'});
              jQuery('.floatingOpen').animate({'right': '0px'});
            }, 3000);
            jQuery('.floatingOpen').click(
              function () {
                jQuery('.floatingImg').animate({'right': '0px'});
                jQuery('.floating').removeClass('slideImg');
                jQuery('.floatingOpen').animate({'right': '-45px'});
                setTimeout(function() {
                  jQuery('.floating').addClass('slideImg');
                  jQuery('.floatingImg').animate({'right': '-170px'});
                  jQuery('.floatingOpen').animate({'right': '0px'});
                }, 3000);
              }
              );
          }
        });
}

