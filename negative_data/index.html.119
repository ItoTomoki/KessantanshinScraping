                    $('.lightBoxWrap').prepend(addBox);
                    
                }else{
                    var addItem = '<li class="relatedLinkBox__item"><a href="'+link+'" target="'+windowtype+'" ><img src="'+img+'" alt="'+alt+'" class="banner over" /></a><p class="relatedLinkBox__txt">'+linktext+'</p></li>';

                    addItemSP += '<div class="items_innnerBox"><a href="'+link+'" class="link'+keyNum+'" target="'+windowtype+'"><img src="'+img+'" alt="'+alt+'" class="banner over" /></a><p class="relatedLinkBox__txt">'+linktext+'</p></div>';
