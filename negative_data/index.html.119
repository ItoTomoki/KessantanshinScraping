    $('#content').find('.bnr-container').each(function(){
      var $this = $(this)
        , $slider = $this.children('div.mask')
        , has_direction_nav = ($this.data('directionnav') === true) ? true : false;
      $slider.flexslider({
        selector          : 'ul.slides > li',
        animation         : 'slide',
        animationLoop     : true,
        slideshow         : true,
        slideshowSpeed    : 4500,
        controlsContainer : $this,
        directionNav      : false
      });
      if(has_direction_nav) {
        $('<a class="prev" href="#"></a>')
          .on('click', function(){
            $slider.flexslider('prev');
            return false;
          })
          .insertBefore($this.find('ol.flex-control-nav'));

        $('<a class="next" href="#"></a>')
          .on('click', function(){
            $slider.flexslider('next');
            return false;
          })
          .insertAfter($this.find('ol.flex-control-nav'));
      }
    });
