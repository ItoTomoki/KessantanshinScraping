<script> 
(function($) {
$(document).ready(function(){
	 
	var $win = $(window), 
		$pviewport = $('.pickupBanner'),
		$pflipsnap = $('.pickupBanner .flipsnap'),
		$pitem = $('.pickupBanner .item');
 
	function pflipsnapIni(){ 
		var winWidth = $win.width();
		var itemWidth = $pitem.width();

		var pfs = Flipsnap($pflipsnap.get(0), {
			distance: itemWidth,
			maxPoint: 2		});
		$pflipsnap.data('fs', pfs);

		$win.bind('resize', function(){
			prefresh();
		}).trigger('resize');
	}
 
	function prefresh(){ 
		var winWidth = $win.width();
		var itemWidth = $pitem.width();
		var pfs = $pflipsnap.data('fs');

		pfs.distance = itemWidth;

		pfsmax = 2;
		if(winWidth <= 640){ pfsmax = 5; }
		pfs.maxPoint = pfsmax;

		pfs.refresh();
    }
 
	pflipsnapIni(); 
 
	var time = 5000; 

	function pstartTimer(){
		pflipTimer = setInterval(function(){
			var pfs = $pflipsnap.data('fs');
			if(pfs.hasNext()){ pfs.toNext(); }
			else{ pfs.moveToPoint(0); }
		} , time);
	}
	pstartTimer();
 	
});
})(jQuery);
</script> 
  
 
 
