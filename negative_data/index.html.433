				pagerFlag = true;
			})
		},
		onSlideBefore: function($slideElement, oldIndex, newIndex){
		var count = slider.getSlideCount() - 1;
			if(newIndex == 0 && current == count && pagerFlag == false) {
				slider2.delay(200).goToNextSlide();
			} else {
				slider2.delay(200).goToSlide(newIndex);
			}
		current = slider.getCurrentSlide();
		pagerFlag = false;
		},
		onSlideAfter: function(){
			cover.show().delay(200).fadeOut(1);
			slider.startAuto();
		}
	});
