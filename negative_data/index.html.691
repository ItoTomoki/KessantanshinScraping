<script src="common/js/jquery.tile.min.js"></script>	
<script>

//リリース表示
$('section.caution,section.notice').hide();
$.get('/chtml/special/top_list.html',{}).done(function(html) {
	if (0 < html.length) {
		$('ul.cautions').html(html);
		$('section.caution').show();
	}
});
$.get('/chtml/news/top_list.html',{}).done(function(html) {
	$('ul.newsContainer').html(html);
});
$.get('/chtml/news/ir_home_list.html',{}).done(function(html) {
	$('ul.irContainer').html(html);
});
$.get('/chtml/info/top_list.html',{}).done(function(html) {
	if (0 < html.length) {
		$('ul.infoContainer').html(html);
		$('section.notice').show();
	}
	nrImgPosition();
	windowSize();
});

window.onload = function(){
    windowSize();
	//nrImgPosition();
};
window.onresize = function(){
	nrImgPosition();
	windowSize();
};
//ニュースリリース高さ調整
function windowSize(){
  var w = $(window).width(),
  tabW = 768,
  smpW = 480;
  if (w <= smpW) {
		$('.newsContainer p').tile(1);
		$('.irContainer p').tile(1);
		$('.infoContainer p').tile(1);
  } else if (w <= tabW && smpW < w ) {
		$('.newsContainer p').tile(2);
		$('.irContainer p').tile(2);
		$('.infoContainer p').tile(2);
  } else {
		$('.newsContainer p').tile(4);
		$('.irContainer p').tile(4);
		$('.infoContainer p').tile(4);
  }
}
//ニュースリリース画像位置調整
function nrImgPosition(){
	var imgW = $('.newsItemWrap figure > img').width(),
	figH = imgW * .76666;
	$('.newsItemWrap figure').each(function(){
		$(this).css({height:figH});
	});
}

var ua = navigator.userAgent.toLowerCase(); 
// iPhone
var isiPhone = (ua.indexOf('iphone') > -1);
// iPad
var isiPad = (ua.indexOf('ipad') > -1);
// Android
var isAndroid = (ua.indexOf('android') > -1) && (ua.indexOf('mobile') > -1);
// Android Tablet
var isAndroidTablet = (ua.indexOf('android') > -1) && (ua.indexOf('mobile') == -1);

if(isiPhone) {
	$('html').addClass('ios');
	$('html').addClass('sp');
	$('html').addClass('mobile');
}
if(isiPad) {
	$('html').addClass('ios');
	$('html').addClass('tb');
	$('html').addClass('mobile');
}
if(isAndroid) {
	$('html').addClass('android');
	$('html').addClass('sp');
	$('html').addClass('mobile');
}
if(isAndroidTablet) {
	$('html').addClass('android');
	$('html').addClass('tb');
	$('html').addClass('mobile');
}
$(function(){
    $('.ir-contents-match').matchHeight();
    topNav();
    $(window).on('resize',function(){
        topNav();
    })
	if($('html').hasClass('mobile')){
		slideImgs();
	}
	$('.pageScroll').on('click',function(){
		$("html,body").animate({scrollTop:gnavPoint},800);
	})
});
//gnav調整
function topNav(){
    var windowH = $(window).height(),
    headerH = $('#header').outerHeight()
    gnav2H = $('#g_navi2').outerHeight(),
    gnavPoint = windowH - (headerH + gnav2H);
    console.log(gnavPoint);
    $(window).scroll(function () {
        if($(window).scrollTop() >= gnavPoint) {
            $('#g_navi').addClass('app');
            $('#g_navi2').addClass('disApp');
 			$('.pageScroll').addClass('disApp');
 			$('#kvWrap h2').removeClass('active');
       } else {
            $('#g_navi').removeClass('app');
            $('#g_navi2').removeClass('disApp');
			$('#kvWrap h2').addClass('active');
        }
    });
}
//mobileSlider
function slideImgs(){
	var $interval = 4000,
	$fade_speed = 2000;
	$('#mobileKv li').hide();
	$('#mobileKv li:first').addClass('active').show();
	setInterval(function(){
		var $active = $('#mobileKv li.active'),
		$next = $active.next('li').length?$active.next('li'):$('#mobileKv li:first');
		$active.fadeOut($fade_speed).removeClass('active');
		$next.fadeIn($fade_speed).addClass('active');
	},$interval);
}

</script>	
    
