  <script>
  $(function(){
  	var elem = $('#li-for-panel-1,#li-for-panel-2');
  	var pnl = $('#panel-1 .main,#panel-2 .main');
  	elem.on('click',function(){
  		var index = elem.index(this);
  		for(var i = 0,len = elem.length;i < len;i ++){
  			if(0 === index){
  				elem.eq(0).find('label.panel-label').addClass('selected');
  				if(elem.eq(1).find('label.panel-label').hasClass('selected')){
  					elem.eq(1).find('label.panel-label').removeClass('selected');
  				}
  				pnl.eq(0).css({display: 'block'});
          /*pnl.eq(0).css({max-height: : 'none'});*/
  				pnl.eq(1).css({display: 'none'});
          /*pnl.eq(1).css({max-height: : '0'});*/

  			}else{
  				if(elem.eq(0).find('label.panel-label').hasClass('selected')){
  					elem.eq(0).find('label.panel-label').removeClass('selected');
  				}
  				elem.eq(1).find('label.panel-label').addClass('selected');
  				pnl.eq(0).css({display: 'none'});
          /*pnl.eq(0).css({max-height: : '0'});*/
  				pnl.eq(1).css({display: 'block'});
          /*pnl.eq(1).css({max-height: : 'none'});*/

  			}
  		}
  	});
  });
