$(function(){
	function timer(){
		setTimeout(function(){
				if($('#hdWrap').html() === ''){
					timer();
				}else{
					addNews();
				}
		},1000);
	}
	function addNews(){
		var tag = $('#hdWrap'),str = '',news = [];
		/*for(var i = 0,len = tag.length;i < len;i++){
			if(tag[i].tagName === 'DIV'){
				news.push(tag[i]);
			}
		}*/
		news = tag.find('.news');
		for(var i = 0;i < 5;i ++){

			var title = news.eq(i).find('.href_with_thum').html();
			var date = news.eq(i).find('.date').html();
			title = title.replace(/<br>/g,'');
			str += '<dt>' + date + '</dt><dd>' + title + '</dd>'
		}
		$('#hole').html(str);
	}
	timer();
});
