<script src='/common-new/js/pages/index.js'></script>
<script src='/common-new/js/pages/index-movie.js'></script>
<!-- vue -->

<script>
	$.apiCallback.set = function () {
		$('.l-whats-new-item').slick({
			accessibility: false,
			dots: true
		});
	};

	$.apiCallback.set = function () {
		// v-wait.on > v-wait
		Array.prototype.slice
		.call(document.querySelectorAll('.v-wait'))
		.forEach(function (self) {
			self.classList.remove('on');
		})
	};

	$.apiCallback.set = function () {
		// ==================================================
		// 高さを揃える
		// ==================================================
		//
		$(".js-height-line-1.row>li").heightLine({
		});
		$(".js-height-line-2.row>li").heightLine({
		});
		var timer = false;
		$(window).resize(function () {
			if (timer !== false) {
				clearTimeout(timer);
			}
			timer = setTimeout(function () {
				$(".js-height-line-1.row>li").heightLine({
				});
				$(".js-height-line-2.row>li").heightLine({
				});
			}, 200);
		});
	};

</script>

</body>
</html>
