<script type="text/javascript">
(function($){
    $(function(){
        // ニュースリリース & 緊急告知
        var tfNews = new TopicsFetcher({
            topicsGroupId: 6,
            cnt: 100,
            puts: function(pageInfo, topicsList) {
                // ニュースリリース用整形
                function process(topic) {
                    var html;
                    var target;
                    if(topic.ext_col_02 == '::01::'){
                        target = ' target="_blank" ';
                    }else{
                        target = '';
                    }

                    var icon;
                    switch (topic.contents_type) {
                        case 36:
                            icon = '/files/user/news/res/image/icn_campaign.gif';
                            break;
                        case 49:
                            icon = '/files/user/news/res/image/icn_item.gif';
                            break;
                        case 50:
                            icon = '/files/user/news/res/image/icn_group.gif';
                            break;
                        case 83:
                            icon = '/files/user/news/res/image/icn_campaign.gif';
                            break;
                        case 84:
                            icon = '/files/user/news/res/image/icn_item.gif';
                            break;
                        case 85:
                            icon = '/files/user/news/res/image/icn_group.gif';
                            break;
                        default:
                            icon = '/files/user/news/res/image/icn_other.gif';
                            break;
                    }
                    html = '<dl class="mt10">' +
                           '<dt class="fwb">' + TopicsFetcher.formatYmd(topic.ymd) + '<img src="' + icon + '" alt="' + topic.contents_type_nm + '" width="67" height="16" class="ml10"></dt>' +
                           '<dd><a href="' + TopicsFetcher.getTopicsDetailUrl(topic) + '" ' + target + '>' + topic.subject + '</a></dd>' +
                           '</dl>';
                    return html;
                };

                // 緊急告知用整形
                function processForUrgent(topic) {
                    var html;
                    var target;
                    if(topic.ext_col_02 == '::01::'){
                        target = ' target="_blank" ';
                    }else{
                        target = '';
                    }

                    html = '<dl class="clearfix">' +
                           '<dt>' + TopicsFetcher.formatYmd(topic.ymd) + '</dt>' +
                           '<dd><a href="' + TopicsFetcher.getTopicsDetailUrl(topic) + '" ' + target + '>' + topic.subject + '</a></dd>' +
                           '</dl>';
                    return html;
                }
                var max_cnt = 5;
                if(topicsList.length < 5){
                    max_cnt = topicsList.length;
                }

                for (var i = 0; i < topicsList.length; i++) {
                    // ext_col_25が01のもの（緊急告知）を表示
                    if (topicsList[i].ext_col_25 && topicsList[i].ext_col_25.replace(/:/g, '') === '01') {
                        $('.headerInfo').append(processForUrgent(topicsList[i]));
                    }
                }
                for (var i = 0; i < max_cnt; i++) {
                    $('#js-newsrelease .txt').append(process(topicsList[i]));
                }
            }
        }).run();

        // お知らせ
        var tfIrOshirase = new TopicsFetcher({
            topicsGroupId: 7,
            cnt: 3,
            puts: function(pageInfo, topicsList) {
                function process(topic) {
                    var html;
                    var target;
                    if(topic.ext_col_02 == '::01::'){
                        target = ' target="_blank" ';
                    }else{
                        target = '';
                    }

                    html = '<dl>' +
                           '<dt class="fwb">' + TopicsFetcher.formatYmd(topic.ymd) + '</dt>' +
                           '<dd><a href="' + TopicsFetcher.getTopicsDetailUrl(topic) + '" ' + target + '>' + topic.subject + '</a></dd>' +
                           '</dl>';
                    return html;
                };
                for (var i = 0; i < topicsList.length; i++) {
                    $('.col4 .unit03 ul').append(process(topicsList[i]));
                }
            }
        }).run();

        // IRニュース
        var tfIrNews = new TopicsFetcher({
            topicsGroupId: 5,
            cnt: 5,
            puts: function(pageInfo, topicsList) {
                function process(topic) {
                    var html;
                    var target;
                    if(topic.ext_col_02 == '::01::'){
                        target = ' target="_blank" ';
                    }else{
                        target = '';
                    }

                    html = '<dl class="mt10">' +
                           '<dt class="fwb">' + TopicsFetcher.formatYmd(topic.ymd) + '</dt>' +
                           '<dd><a href="' + TopicsFetcher.getTopicsDetailUrl(topic) + '" ' + target + '>' + topic.subject + '</a></dd>' +
                           '</dl>';
                    return html;
                };
                for (var i = 0; i < topicsList.length; i++) {
                    $('#js-irnews .txt').append(process(topicsList[i]));
                }
            }
        }).run();
    });
})(jQuery);
</script>

