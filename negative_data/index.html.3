	/* Set the width of the side navigation to 250px and the left margin of the page content to 250px and add a black background color to body */
	function openNav() {
			document.querySelector("#mySlideNav").style.opacity = "1.0";
			document.querySelector("#mySlideNav").style.maxHeight = "none";
			document.querySelector("#mySlideNav").style.marginTop = "20px";
			document.querySelector("#slideout-menu_sp_open").style.display = "none";
			document.querySelector("#slideout-menu_sp_close").style.display = "inline";
			document.querySelector(".slidenav").style.opacity = "1";
			document.querySelector(".contents-area").style.display = "none";
			document.querySelector("#footerLinks").style.display = "none";
			document.querySelector("#footer").style.display = "none";
	}

	/* Set the width of the side navigation to 0 and the left margin of the page content to 0, and the background color of body to white */
	function closeNav() {
			document.querySelector("#mySlideNav").style.marginTop = "20px";
			document.querySelector("#mySlideNav").style.maxHeight = "0";
			document.querySelector("#slideout-menu_sp_close").style.display = "none";
			document.querySelector("#slideout-menu_sp_open").style.display = "inline";
			document.querySelector(".slidenav").style.opacity = "0";
			document.querySelector(".contents-area").style.display = "block";
			document.querySelector("#footerLinks").style.display = "block";
			document.querySelector("#footer").style.display = "block";
	}

	var width = window.innerWidth;
	window.onresize = function(){
		if(this.window.innerWidth != width){
			if(window.innerWidth >= "769"){
				document.querySelector("#slideout-menu_sp_open").style.display = "none";
				document.querySelector("#slideout-menu_sp_close").style.display = "none";
				document.querySelector("#mySlideNav").style.maxHeight = "0";
				document.querySelector("#mySlideNav").style.marginTop = "0";
			} else if(window.innerWidth <= "769"){
				document.querySelector(".inquiry #slideout-menu_sp_open").style.display = "inline";
				document.querySelector("#slideout-menu_sp_close").style.display = "none";
				document.querySelector("#mySlideNav").style.maxHeight = "0";
				document.querySelector(".slidenav").style.opacity = "1";
				document.querySelector(".contents-area").style.display = "block";
			}
		}
	};

	var acc = document.getElementsByClassName("accordion-sp");
	for(var i = 0; i < acc.length; i++){
		acc[i].onclick = function(){
			this.classList.toggle("active");
			this.nextElementSibling.classList.toggle("show");
		}
	}
