				<script type="text/javascript">
					function focusSeachWord(isFocus){
						obj =  $('#searchWord');
						if (obj.val() == '' || obj.val() == '商品番号を入力'||obj.val() == 'キーワードを入力'){
							if (isFocus){
								// onfocus 初期表示値を消す
								obj.val('');
								obj.css("color","#333333");
							} else {
								// off focus(onblur) 初期表示する
								if( $('#searchMethodProductNo').is(':checked')) obj.val('商品番号を入力');
								else obj.val('キーワードを入力');
								obj.css("color","#999999");
							}
						}
						// 2013.12.19 IEでカーソルが左に寄る不具合の修正 start
						//obj.val(obj.val().replace(/\u002f/g,""));
						if (!isFocus) {
							obj.val(obj.val().replace(/\u002f/g,""));
						}
						// 2013.12.19 IEでカーソルが左に寄る不具合の修正 end
					}
					focusSeachWord(false);
				</script>

