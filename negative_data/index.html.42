<script type="text/javascript">
(function($){
  var timer;
  var $img = [];
  
  $(function(){
      $img.push($('<img/>').attr({ src: '/images/50th_anniv.jpg', width: 950, height: 350 }).css({
        position: 'absolute',
        top: 0,
        left: 0
      }).prependTo($('#main-image')));
    for(var i = 2; i < 8; i++){
      $img.push($('<img/>').attr({ src: './images/main_image0' + i + '.jpg', width: 950, height: 350 }).css({
        position: 'absolute',
        top: 0,
        left: 0
      }).prependTo($('#main-image')));
    }
    $img[0].clone().prependTo($('#main-image'));
    
    $('#main-image .nav a').each(function(i){
      $(this).hover(function(){
        if(timer)
          clearTimeout(timer);
        var length = $('#main-image > img').length;
        $img[i+1].clone().insertBefore($('#main-image > .nav')).css({ opacity: 0 }).animate({
          opacity: 1
        }, 500, function(){
          if($('#main-image > img').length == length + 1){
            $('#main-image > img:lt(' + ($('#main-image > img').length - 1) + ')').remove();
          }
        });
      },function(){
        timer = setTimeout(function(){
          var length = $('#main-image > img').length;
          $img[0].clone().insertBefore($('#main-image > .nav')).css({ opacity: 0 }).animate({
            opacity: 1
          }, 500, function(){
            if($('#main-image > img').length == length + 1){
              $('#main-image > img:lt(' + ($('#main-image > img').length - 1) + ')').remove();
            }
          });
        }, 750);
      });
    });
  });
})(jQuery);
</script>
