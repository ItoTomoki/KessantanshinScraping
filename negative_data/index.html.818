			function new_disp(max_date){
				var w_today	= new Date();
				
				// 日付オブジェクトより年、月、日を取得し合体 //
				var w_year		= w_today.getFullYear();
				var w_date		= w_today.getMonth() + 1;
				var w_day		= w_today.getDate();
				var now_date	= w_year * 10000 + w_date * 100 + w_day;

				// 指定された日付が来るまで「new!!」を表示する。 //
				if( now_date <= max_date){
					document.write('<font color="#FF0000">NEW</font>');
				}else{
					document.write('');
				}
			}
		</script>


