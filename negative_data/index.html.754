	});
	$(function() {
		$("#popup_close").click(function(){
		$("#popup").animate({bottom:'18rem', opacity:0}, 400);
		$("#popup_close").animate({opacity:0}, 400);
		$("#overlay").fadeOut(400);
		});
		$("#popup_close02").click(function(){
		$("#popup02").animate({bottom:'10rem', opacity:0}, 400);
		$("#popup_close02").animate({opacity:0}, 400);
		$("#overlay02").fadeOut(400);
		});
	});
</script>
