	<script src="//cdn.jsdelivr.net/jquery.slick/1.6.0/slick.min.js"></script>
	<script type="text/javascript">
		$(function() {//Slider
			this.num_images = $('#kv').find("img").size();
			this.num_loaded_images = 0;
			this.is_autoplaying = false;
			$('#kv').slick({
				//autoplaySpeed: 5000,
				autoplaySpeed: 6000,
				dots: true,
				lazyLoad: 'progressive',
			});
			self = this;
			$('#kv').on("lazyLoaded", function(events, slick, image, imageSource) {
				if (self.num_loaded_images < self.num_images) {
					self.num_loaded_images += 1;
				}
				if (!self.is_autoplaying && (self.num_loaded_images === self.num_images)) {
					$('#kv').slick("slickPlay");
					self.is_autoplaying = true;
				}
			});
		});
		$( function () {
			// タブ
			$( '.tab-title li' ).on( 'click', function () {
				var th = $( this ).index(); // 何番目のタブがクリックされたか
				var $tab_title = $( '.tab-title li' );
				var $tab_contents = $( '.tab-contents .tabList' );
				$tab_title.removeClass( 'select' );
				$tab_title.eq( th ).addClass( 'select' );
				$tab_contents.removeClass( 'active' );
				$tab_contents.eq( th ).addClass( 'active' );
			} );
			// M&Aニュース一覧を取得
			$.ajax( {
				type: 'GET',
				url: 'https://www.nihon-ma.co.jp/news/feed/',
				dataType: 'xml',
				success: function ( xml ) {
					$( xml ).find( "item" ).each( function ( i ) {
						// 表示件数の設定
						if ( i >= 7 ) {
							return false;
						}
						var item_title = $( this ).find( "title" ).text(); //記事タイトル
						var item_link = $( this ).find( "link" ).text(); //リンク
						var item_date = $( this ).find( "pubDate" ).text(); //日付
						// HTML生成
						if ( item_title != "" ) {
							$( "<li class='manews' title='" + item_title + "'><a href='" + item_link + "'><span class='day'>" + dateFormat( item_date ) + "</span><span class='innertext'>" + item_title + "</span></a></li>" ).appendTo( "ul.tab03" );
						}
					} );
					$( "<li><a href='news/' class='linkBtn color03'>M&amp;A NEWS</a></li>" ).appendTo( "ul.tab03" );
				},
				error: function () {
					$( "<li class='err'>記事を取得できませんでした。</li>" ).appendTo( "ul.tab03" );
				}
			} );
			// fade box
			//imgFade( "#bnrNavi li" );
			// colorbox*
			$( '.youtube' ).colorbox( {iframe: true, innerWidth: 853, innerHeight: 480, maxWidth: '98%', maxHeight: '98%'} );
		} );

		// 日付の整形
		function dateFormat( date ) {
			var dateStr = new Date( date ); // 日付の変換
			var newsMonth = dateStr.getMonth() + 1; // 月の取得
			var newsDate = dateStr.getDate(); // 月の取得
			return [
				dateStr.getFullYear(),
				( '0' + newsMonth ).slice( -2 ), // 月表示2桁変換
				( '0' + newsDate ).slice( -2 ) // 日表示2桁変換
			].join( '/' );
		}
	</script>
