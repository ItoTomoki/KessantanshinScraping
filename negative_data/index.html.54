<script>
$(function(){
	$('.ir_tab').click(function(){
		chTab(this);
	});
});
function chTab(elm){
	$('.ir_tab').each(function(){
		$(this).removeClass('ir_tabOn');
		$(this).addClass('ir_tabOff');
	});
	$(elm).addClass('ir_tabOn');
	$(elm).removeClass('ir_tabOff');
	
	if($(elm).attr('no') == 1){
		$('#ir_news1').removeClass('noDisp');
		$('#ir_news2').addClass('noDisp');
	}else{
		$('#ir_news1').addClass('noDisp');
		$('#ir_news2').removeClass('noDisp');
	}
}
</script>

<script>
function chTopix(num){
	if(num == 1){
		$("#tab1").addClass('tabActive');
		$("#tab2").removeClass('tabActive');
		TabLink_onclick('newt',0);
		$('#goList').attr('href', './release.html');
		return false;	
	}else if(num == 2){
		$("#tab2").addClass('tabActive');
		$("#tab1").removeClass('tabActive');
		TabLink_onclick('ir',1);
		$('#goList').attr('href', './ir/news.html');
		return false;
	}
	return false;
}

</script>
