<script type="text/javascript">
	$(function(){
		$('.slider-for').slick({
			// autoplay:true,
			arrows: true,
			centerMode: true,
			infinite: true,
			asNavFor: '.slider-nav',
			variableWidth: true
		});
		$('.slider-nav').slick({
			slidesToShow: 3,
			infinite: true,
			asNavFor: '.slider-for',
			focusOnSelect: true,
			centerMode: true,
			centerPadding: '10px',
			arrows: true
		});
		$('.bn-slider').slick({
		  infinite: true,
		  arrows: true,
		  slidesToShow: 4,
		  slidesToScroll: 1
		});
		$("a").on("click",function(){
			if($(this).attr("href").indexOf("/kanto/tokyo/") > -1){
				$.cookie('area', 'tokyo', {expires:24*60*60*1000*24, path:'/'});
			}
		});
		
		var opentab = function(){
			$('.tabpage', $('#navi')).each(function(i, e){
				if ((i+1) == opend){
					$(e).fadeIn();
				}
				else{
					$(e).fadeOut();
				}
			});
		};
		var closemenu = function(){
			if (!opend){
				$('#navi .tabpage').fadeOut();
			}
		};
		$('#navi ul a').hover(function(){
			var t = $(this).attr('gnaviindex')
			if (t && (opend !== t)){
				opend = $(this).attr('gnaviindex');
				setTimeout(opentab, 100);
			}
		}, function(){
			opend = '';
			setTimeout(function(){
						closemenu()}, 1000);
		});
		$('#navi .tabpage').hover(function(){
			opend = 'aaa';
			//opend = 'a';
		}, function(){
				opend = '';
			setTimeout(function(){
				closemenu();
			}, 500);
		});
	});
</script>


