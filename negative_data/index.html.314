<script type="text/javascript">
	
$(document).ready(function(){
        $.get("/topics/getTop4Topics?_=" + new Date().getTime(),function(d){
            var d = JSON.parse(d);
            
            $("#topicList").empty();

			// Show topics 
			for(var i in d){
				var temp = d[i];
				var topic  = $("#topicMaster>li").clone();
				topic.find(".topics_title").text(temp["topics_title"]);
				if(temp["end_date"] != ""){
					topic.find(".topics_date").html(temp["start_date"]+"～<br>"+temp["end_date"]);
				}else{
					topic.find(".topics_date").text(temp["start_date"]);
				}
				if(temp["topics_type"] == "作成しない"){
					if((temp["text_link"]) != ""){
						topic.find(".topic_link").attr("href",temp["text_link"]);    
					}else if((temp["pdf_link"]) != ""){
						topic.find(".topic_link").attr("href",temp["pdf_link"]);
					}
				}else{
					topic.find(".topic_link").attr("href",temp["url"]);
				}

				var categoryArray = {"イベント":"is-event","キャンペーン":"is-campaign","お知らせ":"is-info"};
				topic.find(".topics_category").addClass(categoryArray[temp["category"]]);
				topic.find(".topics_category").text(temp["category"]);

				if(temp["site"] == "HD"){
					temp["thumbnail"] = "/uploads/media/2017/01/20170110181456.png";
				}else if(temp["site"] == "SP"){
					temp["thumbnail"] = "/front/corporate/common/img/topics_05.png";
				}
				
				topic.find(".topic_image").attr("src",temp["thumbnail"]);
				
				
				$("#topicList").append(topic);
			}
        });
	
	
	$.get("/maintenance/getData",function(data){
		$(".maintenance-list").empty();
		
		var d = JSON.parse(data);
		d	  = d["info"];
		for(var i in d){
			var info  = $("#maintenanceMaster>div").clone();
			var temp = d[i];
			
			if(temp["urgent"] == 0 || temp["urgent"] == undefined){
				info.find(".tag").remove();
			}else{
				info.addClass("is-caution");
			}
			info.find(".body").text(temp["title"]);
			if(temp["end_date"] != ""){
				info.find(".date").html(temp["start_date"]+"～"+temp["end_date"]);
			}else{
				info.find(".date").text(temp["start_date"]);
			}
			if(temp["details_exist"] == "ある"){
				info.find("a").attr("href","maintenance/"+temp["pagename"]).addClass("underbar");
			}else if(temp["details_exist"] == "作成する"){
				info.find("a").attr("href",temp["pagename"]).addClass("underbar");
			}else if(temp["details_exist"] == "作成しない" && temp["pagename"] != ""){
				info.find("a").attr("href",temp["pagename"]).addClass("underbar nopdfbg");
			}
			
			$(".maintenance-list").append(info);
		}
		if(d.length == 0){$("#maintenanceBlock,.header-maintenance").remove();}
		
		// Put Urgent Info on top
		$($(".maintenance-list__item.is-caution").get().reverse()).each(function(k,v){
		 $(".maintenance-list").prepend(v);
		});
		
	});
	
