<script type="text/javascript">
$(function(){
    var sets = [], temp = [];
    $('#blog_area .RightContents .list .box table').each(function(i){
        temp.push(this);
        if (i % 3 == 2){ // 1行の数を指定し、1を引いた数を右に指定
            sets.push(temp);
            temp = [];
        }
    });
    if (temp.length) sets.push(temp);

    $.each(sets, function(){
        $(this).flatHeights();
    });
});
</script>

