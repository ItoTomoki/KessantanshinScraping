 <script type="text/javascript">
 $(function() {
     $('.el').corner("6px");
     $('.el2').corner("8px");
     var showFlug = false;
     var topBtn = $('#backtotop');
     topBtn.css('bottom', '-100px');
     var showFlug = false;
     $(window).scroll(function() {
         if ($(this).scrollTop() > 100) {
             if (showFlug == false) {
                 showFlug = true;
                 topBtn.stop().animate({
                     'opacity': 1,
                     'bottom': '20px'
                 },
                 200);
             }
         } else {
             if (showFlug) {
                 showFlug = false;
                 //topBtn.stop().animate({'bottom' : '-100px'}, 200); 
                 //topBtn.stop().animate({'bottom' : '100px','opacity':0}, 200,function(){$(this).css('bottom' , '-100px')}); 
                 topBtn.stop().animate({
                     'bottom': '-100px',
                     'opacity': 0
                 },
                 200);
             }
         }
     });
     topBtn.click(function() {
         $('body,html').animate({
             scrollTop: 0
         },
         500);
         return false;
     });
 });
 </script>
