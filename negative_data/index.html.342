<!-- JavaScript(original) BGN -->

<script src="js/jquery.matchHeight.js"></script>
<script src="js/slick.js"></script>
<script>
jQuery(document).ready(function($) {
$('.item').matchHeight();
var flag;
function check() {
if($('html').width() < 601 ){
if(flag){
$('.multiple-items').slick('unslick');
flag = 0;
}} else {
$('.multiple-items').slick({
infinite: true,
slidesToShow: 4,
slidesToScroll: 4,
dots: false,
autoplay:true,
autoplaySpeed:5000
});
flag = 1;
}}
$(function(){
check();
});
$(window).resize(function() {
check();
});
});
</script>

