$(function() {
    //タブクリック時の処理
    $('#news .tabbox ul li').click(function() {
        //.index()を使いクリックされたタブの順番を変数indexに代入する
        var index = $('#news .tabbox ul li').index(this);
        //指定した全コンテンツを非表示にする
        $('.contentslist').css('display','none');
  //クリックされたタブと同じ順番のコンテンツのみを表示させる
        $('.contentslist').eq(index).css('display','block');
    $('#news .tabbox ul li').removeClass('select');
    $(this).addClass('select')

    });
});

$(function() {
  // 初期位置へ移動
  $('.tabbox span').css({
    width: $('.tabbox .current').outerWidth(),
    left: $('.tabbox .current').position().left
  });
  // マウスオーバーでアニメーション
  $('.tabbox li').mouseover(function(){
    $('.tabbox span').stop().animate({
      width: $(this).outerWidth(),
      left: $(this).position().left}
    ,'fast');
  });
});
</script>

