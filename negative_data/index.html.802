//スクロール
$(function(){
	$('a.page-top[href^=#]').click(function(){
		var speed = 500;
		var href= $(this).attr("href");
		var target = $(href == "#" || href == "" ? 'html' : href);
		var position = target.offset().top;
		$("html, body").animate({scrollTop:position}, speed, "swing");
		return false;
	});
});

//平成
$(function(){
  setYear();
  labelYear();
});
function labelYear()
{
  var o=$('.title em a');
  var l = o.length;
  var year=false;
  for (i=0;i<l;i++) {
    var t = o.eq(i);
    if (t.attr('data')) {
      if (year != t.attr('data')) {
        year = t.attr('data');
        t.closest('li').before('<li class="year">'+t.attr('data')+'</li>');
      }
    }
  }
}
function setYear()
{
  var pattern=/(平成.*年).*期/i;
  var o=$('.title em a');
  var l = o.length;
  for (i=0;i<l;i++) {
    if (m = o.eq(i).html().match(pattern)) {
      o.eq(i).attr('data',strToSingle(m[1]) + '度');
    }
  }
}
function strToSingle(str)
{
  return str.replace(/[Ａ-Ｚａ-ｚ０-９]/g, function(s) {
   return String.fromCharCode(s.charCodeAt(0) - 0xFEE0);
  });
}

