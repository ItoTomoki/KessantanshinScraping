<script type="text/javascript">

         $(function(){
            
            var default_language_cookie_name = 'SEIREN_DEFAULT_LANG';
            var default_language = $.cookie(default_language_cookie_name);
            
            var getBrowserLanguage = function() {
              var ua = window.navigator.userAgent.toLowerCase();
              
              try {
                  return ( 
                     ( navigator.languages 
                    && navigator.languages[0] ) 
                    || navigator.browserLanguage 
                    || navigator.language 
                    || navigator.userLanguage
                 /* || navigator.systemLanguage */
                    ).substr(0,2);
                    
              }
              catch( e ) {
                  //return undefined;
                  return 'ja';
              }
            }
            
            $('.eng').find('a img').on('click',function(){
              $.cookie( default_language_cookie_name , 'en', { expires: 365 , path: "/" });
              location.href = 'english';
            });
            
            //サイト内からのトップページへのアクセスの場合はcookieを保存して終了する。
            if ( document.referrer.indexOf( location.origin ) != -1 ){
               $.cookie( default_language_cookie_name , 'ja', { expires: 365 , path: "/" });
               return;
            }
            
            //サイト外からのトップページアクセスの場合のみ通常処理
            if ( default_language  ){
               if( default_language == 'ja' ){
                  //Cookie期間延長の為に保存しなおす
                  $.cookie( default_language_cookie_name , 'ja', { expires: 365 , path: "/" });
               } else {
                  location.href = 'english';
               }
            } else {
               if ( getBrowserLanguage() == 'ja' ) {
                  $.cookie( default_language_cookie_name , 'ja', { expires: 365 , path: "/" });
               } else {
                  $.cookie( default_language_cookie_name , 'en', { expires: 365 , path: "/" });
                  location.href = 'english';
               }
            }
         });
         

