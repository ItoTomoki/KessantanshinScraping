<script type="text/javascript">
$(function() {
    /***
     *  言語切り替えのリンク設定 20170522 追加
     *
     *  /en/がついていたら、?lang=enに書き換えられる。(.htaccess参照）
     *  英語ページをGoogleにIndexさせるため、従来のCookieを用いた言語切り替えの方法に、
     *  パラメータでの判定を追加。(functions.php参照)
     */
    var href = window.location.href;
    var protocol = window.location.protocol + '//';
    var host = window.location.host;
    var hash = window.location.hash;
    var urlParam = window.location.search.substring(1);

    // URLからプロトコル、ホスト、パラメータを除いたパスを抽出する
    if(urlParam) {
        // ハッシュがある場合は付ける
        if (hash) {
            var pass = href.slice(protocol.length + host.length, -(hash.length + urlParam.length +1));
            pass = pass + hash;
        } else {
            var pass = href.slice(protocol.length + host.length, -(urlParam.length +1));
        }
    } else {
        var pass = href.slice(protocol.length + host.length);
    }

    if(urlParam) {
        // パラメータを扱いやすくするために配列に格納
        var param = urlParam.split('&');
        var paramArray = [];

        for (i = 0; i < param.length; i++) {
            var paramItem = param[i].split('=');
            paramArray[paramItem[0]] = paramItem[1];
        }

        // ?lang=enがついている場合
        if (paramArray.lang == 'en') {
            // パラメータを表示させないよう履歴を置き換える
            window.history.replaceState('','','/en' + pass);
       }
    }

    // 言語切り替えのリンクを設定する
    $('.transLang').each(function() {
        var lng = $(this).attr('data-lang');
       if (lng == 'ja') {
            var link = protocol + host + pass;
            $(this).attr('href', link);
        }
        if (lng == 'en') {

            //金融ADRは英語ページがないので何もさせない 2017/04/04 IPC
            if (href.indexOf('/adr/') != -1) {
              return false;
            }

            var link = protocol + host + '/en' + pass;
            $(this).attr('href', link);
        }
    });


    $('.transLang').click(function() {
        var lng = $(this).attr('data-lang');
        var expire = new Date();
        expire.setTime(expire.getTime() + 1000 * 3600 * 24 * 365);
        if (lng == 'ja') {
            document.cookie = 'language=ja; path=/; expires=' + expire.toUTCString();
        }
        if (lng == 'en') {

			//金融ADRは英語ページがないので何もさせない 2017/04/04 IPC
			var href = window.location.href;
			if (href.indexOf('/adr/') != -1) {
				return false;
			}

            document.cookie = 'language=en; path=/; expires=' + expire.toUTCString();
        }
//        location.reload();
    });
});
</script>
