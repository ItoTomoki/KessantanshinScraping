<script>
$(function() {
	(function() {
		var index = 0;
		var timer = null;
		var interval = 7000;
		var busy = false;
		
		var change = function() {
			if(busy) return;
			busy = true;
			$('.slide:visible').fadeOut(1500, function() {
				$('#slide'+index).fadeIn(1500, function() {
					busy = false;
				});
			});
		};
		
		var loop = function() {
			index = (index + 1) % 3;
			change();
		};
	
		var next = function() {
			clearInterval(timer);
			index = (index + 1) % 3;
			change();
			timer = setInterval(loop, interval);
			return false;
		};
		
		var prev = function() {
			clearInterval(timer);
			index = (index - 1) % 3;
			if(index < 0) index = 2;
			change();
			timer = setInterval(loop, interval);
			return false;
		};
		
		timer = setInterval(loop, interval);
		
		$('#index_a_r').click(next);
		$('#index_a_l').click(prev);
	})();
	
	// $('.slide').hover(function() {
