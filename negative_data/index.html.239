<script type="text/javascript">
  $(document).ready(function(){
    var showcnt = 6;
    $("#listArea div:not(:lt("+showcnt+"))").hide();
    // 絞り込み
    $("#tab li").click(function() {
      // data取得
      var tags = $(this).attr('data-category');
      $("#tab li").removeClass('select');
      $(this).addClass('select');
      // block表示・非表示
      $("#listArea div").hide();
      if(tags == 'news_ir') {
        $("#listArea .news_ir").show();
        $("#listArea div.news_ir:not(:lt("+showcnt+"))").hide();
      } else if(tags == 'news_rel') {
        $("#listArea .news_rel").show();
        $("#listArea div.news_rel:not(:lt("+showcnt+"))").hide();
      } else if(tags == 'news_nwgr') {
        $("#listArea .news_nwgr").show();
        $("#listArea div.news_nwgr:not(:lt("+showcnt+"))").hide();
      } else if(tags == 'news_car') {
        $("#listArea .news_car").show();
        $("#listArea div.news_car:not(:lt("+showcnt+"))").hide();
      } else if(tags == 'news_etc') {
        $("#listArea .news_etc").show();
        $("#listArea div.news_etc:not(:lt("+showcnt+"))").hide();
      } else {
        $("#listArea div").show();
        $("#listArea div:not(:lt("+showcnt+"))").hide();
      }
      return false;
    });
  });
</script>



