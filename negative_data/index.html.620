<script>
$(window).load(function(){
	/* invoke the carousel */ 
	$('#TopCarousel').carousel({
		//インターバル　※ミリ秒1s=1000
		//interval: 7000
		interval: 7000
	});

	/* slide on click */ 
	$('.carousel-linked-nav > li > a').click(function() {
		var item = Number($(this).attr('href').substring(1));
		$('#TopCarousel').carousel(item - 1);
		$('.carousel-linked-nav .active').removeClass('active');
		$(this).parent().addClass('active');
		return false;
	});

	/* Autoplay nav Highlight */
	$('#TopCarousel').bind('slid', function() {
		$('.carousel-linked-nav .active').removeClass('active');
		var idx = $('#TopCarousel .item.active').index();
		$('.carousel-linked-nav li:eq(' + idx + ')').addClass('active');
	});

	/* newsTopics_Tab Control */
	$('#newsTopics_Tab a').click(function (e) {
		e.preventDefault();
		$(this).tab('show');
	});

	index_xmlLoader();

});

//====================================================
//　トップページのCMS処理
//====================================================
function index_xmlLoader(){
	//-----------------
	// 初期値の設定
	//-----------------
	var tdy = new Date();
	yy = tdy.getYear();
	mm = tdy.getMonth() + 1;
	dd = tdy.getDate();
	hh = tdy.getHours();

	var lp = 0;
	var newslp = 0;
	var irlp = 0;

	//-----------------
	// 日付、固有値の設定
	//-----------------
	if (yy < 2000) { yy += 1900; }
	if (mm < 10) { mm = "0" + mm; }
	if (dd < 10) { dd = "0" + dd; }
	if (hh < 10) { hh = "0" + hh; }
	uid = tdy.getTime();
	tdy = yy + "" + mm + "" + dd + "" + hh;

	//-----------------
	// XML読込準備
	//-----------------
	var ldxml = "_data/news.php?"+uid;
	var wrHtml = '';
	var newsHtml = '';
	var irHtml = '';
	var dateFlg = true;
	var httpXML=generateXmlHttp();
	httpXML.open("GET", ldxml , true);
	httpXML.onreadystatechange=function(){
		// readyState == 4で受信完了
		if (httpXML.readyState == 4) {
			sauceXML=httpXML.responseXML;
			loadBlock=sauceXML.getElementsByTagName("content");
			// 各データ処理
			for(var i=0;i<loadBlock.length;i++){
				// 掲載開始日時の取得
				strYY = purseNode(loadBlock[i],'strYY');
				strMM = purseNode(loadBlock[i],'strMM');
				strDD = purseNode(loadBlock[i],'strDD');
				strTT = purseNode(loadBlock[i],'strTT');
				if (strMM < 10) { strMM = "0" + strMM; }
				if (strDD < 10) { strDD = "0" + strDD; }
				if (strTT < 10) { strTT = "0" + strTT; }
				str = strYY + "" + strMM + "" + strDD + "" + strTT;
				// 掲載終了日時の取得
				endYY = purseNode(loadBlock[i],'endYY');
				endMM = purseNode(loadBlock[i],'endMM');
				endDD = purseNode(loadBlock[i],'endDD');
				endTT = purseNode(loadBlock[i],'endTT');
				if (endMM < 10) { endMM = "0" + endMM; }
				if (endDD < 10) { endDD = "0" + endDD; }
				if (endTT < 10) { endTT = "0" + endTT; }
				end = endYY + "" + endMM + "" + endDD + "" + endTT;
				content_title = purseNode(loadBlock[i],'content_title');
				filePDF_box = purseNode(loadBlock[i],'filePDF').split("@");
				filePDF = filePDF_box[0];
				if(filePDF_box[1] != undefined){
					fileSIZE = '　('+filePDF_box[1]+')';
				}else{
					fileSIZE ='';
				}
				linkURL = purseNode(loadBlock[i],'linkURL');
				label = purseNode(loadBlock[i],'label');
				topView = purseNode(loadBlock[i],'topView');
				// フィルタ：掲載期間
				if((str <= tdy) && (tdy < end) && (topView=="する")){

					//----------------------
					// PRESS & Topics：組み立て
					//----------------------
					if(lp<5){
						wrHtml += '<div class="newsHead">'+strYY + "." + strMM + "." + strDD;
						if(label=="PRESS"){
							wrHtml += '<span class="label news">PRESS</span></div>';
						}else if(label=="IR"){
							wrHtml += '<span class="label ir">IR</span></div>';
						}
						if(0<filePDF.length){
							wrHtml += '<div class="newsBody"><a href="_data/news/'+filePDF+'" target="_blank">'+content_title+fileSIZE+'</a></div>';
						}else if(0<linkURL.length){
							wrHtml += '<div class="newsBody"><a href="'+linkURL+'" target="_blank">'+content_title+'</a></div>';
						}else{
							wrHtml += '<div class="newsBody">'+content_title+'</div>';
						}
						lp++;
					}
					//----------------------
					// IR：組み立て
					//----------------------
					if((label=="IR")&&(irlp<5)){
						irHtml += '<div class="newsHead">'+strYY + "." + strMM + "." + strDD;
						irHtml += '<span class="label ir">IR</span></div>';

						if(0<filePDF.length){
							irHtml += '<div class="newsBody"><a href="_data/news/'+filePDF+'" target="_blank">'+content_title+fileSIZE+'</a></div>';
						}else if(0<linkURL.length){
							irHtml += '<div class="newsBody"><a href="'+linkURL+'" target="_blank">'+content_title+'</a></div>';
						}else{
							irHtml += '<div class="newsBody">'+content_title+'</div>';
						}
						irlp++;
					}
					//----------------------
					// PRESS：組み立て
					//----------------------
					if((label=="PRESS")&&(newslp<5)){
						newsHtml += '<div class="newsHead">'+strYY + "." + strMM + "." + strDD;
						newsHtml += '<span class="label news">PRESS</span></div>';

						if(0<filePDF.length){
							newsHtml += '<div class="newsBody"><a href="_data/news/'+filePDF+'" target="_blank">'+content_title+fileSIZE+'</a></div>';
						}else if(0<linkURL.length){
							newsHtml += '<div class="newsBody"><a href="'+linkURL+'" target="_blank">'+content_title+'</a></div>';
						}else{
							newsHtml += '<div class="newsBody">'+content_title+'</div>';
						}
						newslp++;
					}
				}
			}
			// 出力
			document.getElementById("topics").innerHTML = wrHtml;
			document.getElementById("newsData").innerHTML = newsHtml;
			document.getElementById("irData").innerHTML = irHtml;

			info_xmlLoader();
		}
	}
	httpXML.send("");
}

