				// 掲載終了日時の取得
				endYY = purseNode(loadBlock[i],'endYY');
				endMM = purseNode(loadBlock[i],'endMM');
				endDD = purseNode(loadBlock[i],'endDD');
				endTT = purseNode(loadBlock[i],'endTT');
				if (endMM < 10) { endMM = "0" + endMM; }
				if (endDD < 10) { endDD = "0" + endDD; }
				if (endTT < 10) { endTT = "0" + endTT; }
				end = endYY + "" + endMM + "" + endDD + "" + endTT;
				content_title = purseNode(loadBlock[i],'content_title');
				ID = purseNode(loadBlock[i],'ID');
				// フィルタ：掲載期間
				if((str <= tdy) && (tdy < end)){
					// 組み立て
					wrHtml += '<div class="infoHead">'+strYY + "." + strMM + "." + strDD+'</div>';
					wrHtml += '<div class="infoBody"><a href="index_info.php?'+ID+'" target="_blank">&nbsp;'+content_title+'</a></div>';
				}
			}
			wrHead = '<div id="topics" class="tab-pane active">';
			wrFoot = '</div>';
			// 出力
			document.getElementById("info").innerHTML = wrHtml;
		}
	}
	httpXML.send("");
}
</script>
